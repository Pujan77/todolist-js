<%- include("header") -%> 

    <div class="box" id="heading">
        <h1> <%= listTitle %> </h1>
    </div>

    <div class="box">

        <%  for (var i =0; i< toDoList.length; i++){  %>
        <div class="item">
            <input type="checkbox">
            <p><%= toDoList[i] %> </p>
        </div>

        <%   } %>
        
        <form class="item" action="/" method="post">
        <input required="true" type="text" name="newItem" id="names" placeholder="New Item" autocomplete="off" >
        <button type="submit" name="list" value=<%= listTitle %>  >+</button>
    </form>
    </div>
<%- include("footer") -%> 



    

